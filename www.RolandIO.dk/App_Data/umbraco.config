    <BlogPostRepository id="1075" key="d7630912-364d-42c8-8cd8-1d229efc7ece" parentID="1063" level="2" creatorID="0" sortOrder="3" createDate="2016-07-09T15:21:38" updateDate="2016-07-09T15:31:15" nodeName="Blog" urlName="blog" path="-1,1063,1075" isDoc="" nodeType="1059" creatorName="Mark Roland" writerName="Mark Roland" writerID="0" template="1054" nodeTypeAlias="BlogPostRepository">
